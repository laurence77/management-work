# Supabase Configuration - REPLACE WITH YOUR ACTUAL VALUES
SUPABASE_URL=YOUR_SUPABASE_PROJECT_URL
SUPABASE_ANON_KEY=YOUR_SUPABASE_ANON_KEY
SUPABASE_SERVICE_ROLE_KEY=YOUR_SUPABASE_SERVICE_ROLE_KEY

# JWT Secrets - REPLACE WITH YOUR GENERATED SECRETS
JWT_SECRET=YOUR_STRONG_JWT_SECRET_HERE
REFRESH_TOKEN_SECRET=YOUR_STRONG_REFRESH_TOKEN_SECRET_HERE

# Application Configuration
NODE_ENV=development
PORT=3000
DOMAIN=bookmyreservation.org
ADMIN_URL=https://admin.bookmyreservation.org

# Email Configuration (SMTP) - Hostinger
SMTP_HOST=smtp.hostinger.com
SMTP_PORT=465
SMTP_SECURE=true
SMTP_USER=management@bookmyreservation.org
SMTP_PASS=YOUR_EMAIL_PASSWORD
EMAIL_FROM="Celebrity Booking Platform" <management@bookmyreservation.org>
FROM_EMAIL=management@bookmyreservation.org
