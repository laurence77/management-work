{
  "name": "celebrity-booking-platform",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "validate-env": "node validate-environment.js",
    "setup": "./setup-environment.sh",
    "dev": "npm run validate-env && vite",
    "build": "npm run validate-env && tsc && vite build",
    "build:dev": "vite build --mode development",
    "build:analyze": "npm run build && npm run bundle:analyze",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "start:all": "./start-all.sh",
    "stop:all": "./stop-all.sh",
    "admin:install": "cd admin-dashboard && npm install",
    "admin:dev": "cd admin-dashboard && npm run dev",
    "admin:build": "cd admin-dashboard && npm run build",
    "backend:install": "cd backend && npm install",
    "backend:dev": "cd backend && npm run dev",
    "backend:start": "cd backend && npm start",
    "fullstack:install": "npm install && npm run admin:install && npm run backend:install",
    "fullstack:dev": "concurrently \"npm run dev\" \"npm run admin:dev\" \"npm run backend:dev\"",
    "production:build": "npm run build && npm run admin:build",
    "production:deploy": "./deploy-production.sh",
    "production:ssl": "sudo ./setup-ssl.sh",
    "production:verify": "./verify-production.sh",
    "bundle:analyze": "node scripts/bundle-analyzer.js analyze",
    "bundle:quick": "node scripts/bundle-analyzer.js quick",
    "perf:test": "npm run build && npm run preview & sleep 5 && npm run bundle:quick",
    "prepare": "husky install",
    "postinstall": "playwright install"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.0",
    "@radix-ui/react-accordion": "^1.2.0",
    "@radix-ui/react-alert-dialog": "^1.1.1",
    "@radix-ui/react-aspect-ratio": "^1.1.0",
    "@radix-ui/react-avatar": "^1.1.0",
    "@radix-ui/react-checkbox": "^1.1.1",
    "@radix-ui/react-collapsible": "^1.1.0",
    "@radix-ui/react-context-menu": "^2.2.1",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-hover-card": "^1.1.1",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-menubar": "^1.1.1",
    "@radix-ui/react-navigation-menu": "^1.2.0",
    "@radix-ui/react-popover": "^1.1.1",
    "@radix-ui/react-progress": "^1.1.0",
    "@radix-ui/react-radio-group": "^1.2.0",
    "@radix-ui/react-scroll-area": "^1.1.0",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slider": "^1.2.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.1",
    "@radix-ui/react-toggle": "^1.1.0",
    "@radix-ui/react-toggle-group": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.4",
    "@tanstack/react-query": "^5.56.2",
    "@types/lodash": "^4.17.20",
    "@types/multer": "^2.0.0",
    "@types/recharts": "^1.8.29",
    "chart.js": "^4.5.0",
    "chrome-launcher": "^1.2.0",
    "class-variance-authority": "^0.7.1",
    "cloudinary": "^2.7.0",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "date-fns": "^3.6.0",
    "embla-carousel-react": "^8.3.0",
    "framer-motion": "^12.23.10",
    "geoip-lite": "^1.4.10",
    "handlebars": "^4.7.8",
    "input-otp": "^1.2.4",
    "lighthouse": "^12.8.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.462.0",
    "multer": "^2.0.2",
    "next-themes": "^0.3.0",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.5",
    "puppeteer": "^24.15.0",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.3.0",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.0",
    "react-resizable-panels": "^2.1.3",
    "react-router-dom": "^6.26.2",
    "recharts": "^2.15.4",
    "sharp": "^0.34.3",
    "sonner": "^1.5.0",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^0.9.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@eslint/js": "^9.9.0",
    "@jest/globals": "^29.7.0",
    "@playwright/test": "^1.40.0",
    "@tailwindcss/typography": "^0.5.15",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/node": "^22.5.5",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.2.0",
    "eslint": "^9.9.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.0",
    "lovable-tagger": "^1.1.7",
    "postcss": "^8.4.47",
    "prettier": "^3.1.1",
    "tailwindcss": "^3.4.11",
    "ts-jest": "^29.1.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.4.1"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "type-enum": [
        2,
        "always",
        [
          "feat",
          "fix",
          "docs",
          "style",
          "refactor",
          "perf",
          "test",
          "chore",
          "ci",
          "security"
        ]
      ]
    }
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "moduleNameMappings": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts",
      "!src/main.tsx",
      "!src/vite-env.d.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
